    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "immer": "^10.0.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
