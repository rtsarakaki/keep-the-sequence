{
  "name": "the-game-backend",
  "version": "1.0.0",
  "description": "Backend for The Game - Cooperative Card Game",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
